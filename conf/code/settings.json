// Place your settings in this file to overwrite the default settings
{
  "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
  "[dockerfile]": {
    "editor": {
      "defaultFormatter": "ms-azuretools.vscode-docker",
      "formatOnSave": true
    }
  },
  "[json,javascript,markdown,toml,yaml,yml]": {
    "editor": {
      "defaultFormatter": "esbenp.prettier-vscode",
      "formatOnSave": true
    }
  },
  "[jsonc]": {
    "editor": {
      "defaultFormatter": "vscode.json-language-features",
      "formatOnSave": true
    }
  },
  "[python]": {
    "defaultInterpreterPath": "${env:HOMEBREW_PREFIX}/bin/python3",
    "editor": {
      "defaultFormatter": "charliermarsh.ruff",
      "formatOnSave": true,
      "formatOnSaveMode": "file"
    },
    "languageServer": "Default"
  },
  "[shellscript]": {
    "editor": {
      "defaultFormatter": "mads-hartmann.bash-ide-vscode",
      "formatOnSave": true
    }
  },
  "[yaml]": {
    "customTags": [
      "!And",
      "!And sequence",
      "!If",
      "!If sequence",
      "!Not",
      "!Not sequence",
      "!Equals",
      "!Equals sequence",
      "!Or",
      "!Or sequence",
      "!FindInMap",
      "!FindInMap sequence",
      "!Base64",
      "!Join",
      "!Join sequence",
      "!Cidr",
      "!Ref",
      "!Sub",
      "!Sub sequence",
      "!GetAtt",
      "!GetAZs",
      "!ImportValue",
      "!ImportValue sequence",
      "!Select",
      "!Select sequence",
      "!Split",
      "!Split sequence"
    ]
  },
  "amazonQ.telemetry": false,
  "antigravity": {
    "marketplaceExtensionGalleryServiceURL": "https://marketplace.visualstudio.com/_apis/public/gallery",
    "marketplaceGalleryItemURL": "https://marketplace.visualstudio.com/items"
  },
  "application.shellEnvironmentResolutionTimeout": 120,
  "arduino.useArduinoCli": true,
  "aws.cloudformation.telemetry.enabled": false,
  "aws.telemetry": false,
  "carbon.fontFamily": "Source Code Pro",
  "chat.mcp.gallery.enabled": true,
  "cloudcode": {
    "autoDependencies": "off",
    "enableTelemetry": false
  },
  "cmake": {
    "configureOnOpen": false,
    "ignoreCMakeListsMissing": true,
    "showOptionsMovedNotification": false
  },
  "code-runner.enableAppInsights": false,
  "codeQL.cli.executablePath": "/opt/homebrew/bin/codeql",
  "csharp.suppressDotnetInstallWarning": true,
  "debug.terminal.clearBeforeReusing": true,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor": {
    "acceptSuggestionOnEnter": "on",
    "fontSize": 16,
    "formatOnSave": true,
    "inlineSuggest.enabled": true,
    "snippetSuggestions": "top",
    "suggest.preview": true,
    "suggest.showMethods": true,
    "suggestSelection": "first"
  },
  "esbonio.server.enabled": false,
  "extensions.autoUpdate": true,
  "files": {
    "associations": {
      "*.m": "matlab"
    },
    "exclude": {
      "**/.trunk/*out": true
    },
    "insertFinalNewline": true,
    "trimTrailingWhitespace": true
  },
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true,
    "**/.trunk/*out": true
  },
  "firebase.firebasePath": "/opt/homebrew/bin/firebase",
  "geminicodeassist": {
    "enableTelemetry": false
  },
  "git": {
    "autofetch": true,
    "path": "/opt/homebrew/bin/git"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "gitlens": {
    "advanced.messages": {
      "suppressCommitHasNoPreviousCommitWarning": false,
      "suppressCommitNotFoundWarning": false,
      "suppressFileNotUnderSourceControlWarning": false,
      "suppressGitVersionWarning": false,
      "suppressLineUncommittedWarning": false,
      "suppressNoRepositoryWarning": false
    },
    "telemetry.enabled": false
  },
  "go.diagnostic.vulncheck": "Imports",
  "go.toolsManagement.autoUpdate": true,
  "guides.sendUsagesAndStats": false,
  "json.schemaDownload.trustedDomains": {
    "https://biomejs.dev": true,
    "https://json-schema.org/": true,
    "https://json.schemastore.org/": true,
    "https://raw.githubusercontent.com/": true,
    "https://schemastore.azurewebsites.net/": true,
    "https://www.schemastore.org/": true
  },
  "julia": {
    "enableCrashReporter": false,
    "enableTelemetry": false,
    "executablePath": "/opt/homebrew/bin/julia",
    "symbolCacheDownload": true
  },
  "latex-workshop": {
    "formatting.latex": "latexindent",
    "latex": {
      "recipes": [
        {
          "name": "xelatex",
          "tools": [
            "xelatex"
          ]
        }
      ],
      "tools": [
        {
          "args": [
            "--output-directory=."
          ],
          "command": "xelatex",
          "name": "xelatex"
        }
      ]
    }
  },
  "makefile.extensionOutputFolder": "./.vscode",
  "matlab.lintOnSave": false,
  "metals.javaHome": "/Library/Java/JavaVirtualMachines/temurin-21.jdk/Contents/Home",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "partialDiff.enableTelemetry": false,
  "redhat.telemetry.enabled": false,
  "rest-client.enableTelemetry": false,
  "restructuredtext.linter": {
    "doc8.executablePath": "${env:HOMEBREW_PREFIX}/bin/doc8",
    "rst-lint.executablePath": "${env:HOMEBREW_PREFIX}/bin/doc8",
    "rstcheck.executablePath": "${env:HOMEBREW_PREFIX}/bin/doc8"
  },
  "salesforcedx-vscode-core.telemetry.enabled": false,
  "sarif-viewer.connectToGithubCodeScanning": "off",
  "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
  "snowflake.connectionsConfigFile": "${env:HOME}/.snowflake/connections.toml",
  "terminal": {
    "external.osxExec": "iTerm.app",
    "integrated": {
      "commandsToSkipShell": [
        "language-julia.interrupt"
      ],
      "fontFamily": "'MesloLGS NF'",
      "fontSize": 14,
      "persistentSessionReviveProcess": "never"
    }
  },
  "testExplorer.useNativeTesting": true,
  "textmarker.enableTelemetry": false,
  "vs-kubernetes": {
    "helm-path": "${env:HOMEBREW_PREFIX}/bin/helm",
    "kubectl-path": "${env:HOMEBREW_PREFIX}/bin/kubectl",
    "minikube-path": "${env:HOMEBREW_PREFIX}/bin/minikube",
    "vscode-kubernetes.minikube-path.mac": "${env:HOMEBREW_PREFIX}/bin/minikube"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "workbench": {
    "colorTheme": "Catppuccin Macchiato",
    "editor.enablePreview": false,
    "editorAssociations": {
      "*.ipynb": "jupyter-notebook"
    }
  },
  "workbench.iconTheme": "catppuccin-macchiato",
  "yapf.args": [
    "--style",
    "{based_on_style: google, column_limit: 79}"
  ]
}
