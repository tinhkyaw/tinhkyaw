// Place your settings in this file to overwrite the default settings
{
  "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.defaultFormatter": "eeyore.yapf",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "application.shellEnvironmentResolutionTimeout": 120,
  "arduino.useArduinoCli": true,
  "aws.samcli.lambdaTimeout": 1234,
  "aws.telemetry": false,
  "bazel.buildifierPath": "/Users/tin/.brew/bin/buildifier",
  "carbon.fontFamily": "Source Code Pro",
  "cloudcode.autoDependencies": "off",
  "cloudcode.enableTelemetry": false,
  "cmake.configureOnOpen": false,
  "cmake.ignoreCMakeListsMissing": true,
  "cmake.showOptionsMovedNotification": false,
  "code-runner.enableAppInsights": false,
  "codeQL.cli.executablePath": "${env:HOMEBREW_PREFIX}/bin/codeql",
  "codeQL.telemetry.enableTelemetry": false,
  "csharp.suppressDotnetInstallWarning": true,
  "debug.terminal.clearBeforeReusing": true,
  "docker-explorer.enableTelemetry": false,
  "docsYaml.telemetry.enableTelemetry": false,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggest.showMethods": true,
  "editor.suggestSelection": "first",
  "esbonio.server.enabled": false,
  "extensions.autoUpdate": true,
  "files.associations": {
    "*.m": "matlab"
  },
  "files.exclude": {
    "**/.trunk/*out": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true,
    "**/.trunk/*out": true
  },
  "git.autofetch": true,
  "git.path": "/Users/tin/.brew/bin/git",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "gitlens.telemetry.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "guides.sendUsagesAndStats": false,
  "makefile.extensionOutputFolder": "./.vscode",
  "markdownlint.config": {
    "MD025": {
      "front_matter_title": ""
    },
    "MD028": false
  },
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "matlab.lintOnSave": false,
  "metals.javaHome": "/Library/Java/JavaVirtualMachines/temurin-21.jdk/Contents/Home",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "partialDiff.enableTelemetry": false,
  "python.defaultInterpreterPath": "${env:HOMEBREW_PREFIX}/bin/python3",
  "redhat.telemetry.enabled": false,
  "rest-client.enableTelemetry": false,
  "restructuredtext.linter.doc8.executablePath": "${env:HOMEBREW_PREFIX}/bin/doc8",
  "restructuredtext.linter.rst-lint.executablePath": "${env:HOMEBREW_PREFIX}/bin/doc8",
  "restructuredtext.linter.rstcheck.executablePath": "${env:HOMEBREW_PREFIX}/bin/doc8",
  "sarif-viewer.connectToGithubCodeScanning": "off",
  "shellcheck.executablePath": "/Users/tin/.brew/bin/shellcheck",
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "'MesloLGS NF'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "textmarker.enableTelemetry": false,
  "trunk.trunkGrayOutNonBlockingIssues": false,
  "vs-kubernetes": {
    "helm-path": "${env:HOMEBREW_PREFIX}/bin/helm",
    "kubectl-path": "${env:HOMEBREW_PREFIX}/bin/kubectl",
    "minikube-path": "${env:HOMEBREW_PREFIX}/bin/minikube",
    "vscode-kubernetes.minikube-path.mac": "${env:HOMEBREW_PREFIX}/bin/minikube"
  },
  "vscode-kubernetes.helm-path": "${env:HOMEBREW_PREFIX}/bin/helm",
  "vscode-kubernetes.kubectl-path": "${env:HOMEBREW_PREFIX}/bin/kubectl",
  "vscode-kubernetes.minikube-path": "${env:HOMEBREW_PREFIX}/bin/minikube",
  "workbench.colorTheme": "Solarized Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.schemaStore.enable": false,
  "yaml.schemas": {
    "file:///Users/tin/.vscode/extensions/docsmsft.docs-yaml-1.0.1/dist/toc.schema.json": "/toc\\.yml/i"
  },
  "yapf.args": [
    "--style",
    "{based_on_style: google, column_limit: 79}"
  ]
}
